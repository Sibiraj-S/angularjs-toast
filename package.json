{
  "name": "angularjs-toast",
  "version": "4.0.0",
  "description": "A Simple toast notification service for AngularJS pages",
  "author": "sibiraj-s",
  "license": "MIT",
  "private": true,
  "repository": "https://github.com/sibiraj-s/angularjs-toast.git",
  "bugs": "https://github.com/sibiraj-s/angularjs-toast/issues",
  "homepage": "https://github.com/sibiraj-s/angularjs-toast#readme",
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "build": "grunt build",
    "serve": "grunt serve",
    "lint:js": "eslint . --ignore-path .gitignore",
    "lint:coffee": "grunt coffeelintr",
    "lint": "npm run lint:coffee && npm run lint:js",
    "prepublishOnly": "node ./scripts/prepublish.js",
    "prerelease": "npm run build",
    "release": "sh ./scripts/release.sh",
    "test:watch": "karma start",
    "test": "karma start --single-run"
  },
  "keywords": [
    "angularjs-notify",
    "notification",
    "angularjs",
    "notify",
    "angularjs-toast",
    "toast-notification"
  ],
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "angular": "^1.7.9",
    "angular-animate": "^1.7.9",
    "angular-mocks": "^1.7.9",
    "angular-sanitize": "^1.7.9",
    "chalk": "^4.0.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-angular": "^4.0.1",
    "eslint-plugin-import": "^2.20.2",
    "grunt": "^1.1.0",
    "grunt-babel": "^8.0.0",
    "grunt-browser-sync": "^2.2.0",
    "grunt-coffeelintr": "^1.1.5",
    "grunt-contrib-clean": "^2.0.0",
    "grunt-contrib-coffee": "^2.1.0",
    "grunt-contrib-concat": "^1.0.1",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-cssmin": "^3.0.0",
    "grunt-contrib-uglify": "^4.0.1",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-sass": "^3.1.0",
    "husky": "^4.2.5",
    "jquery": "^3.5.0",
    "karma": "^5.0.2",
    "karma-chrome-launcher": "^3.1.0",
    "karma-jasmine": "^3.1.1",
    "load-grunt-tasks": "^5.1.0",
    "sass": "^1.26.5"
  }
}
